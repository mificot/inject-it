    "dependency injection",
    "provide/inject",
    "provide",
    "inject",
    "injectable",
    "services"
  ],
  "author": "mificot",
  "license": "MIT",
  "files": [
    "dist/**/*"
  ],
  "repository": "https://github.com/mificot/inject-it",
  "engines": {
    "node": "18.x"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "eslint": "^8.32.0",
    "eslint-config-standard-with-typescript": "^33.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.4.1",
    "reflect-metadata": "^0.1.13",
    "ts-jest": "^29.0.5",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4"
  }
